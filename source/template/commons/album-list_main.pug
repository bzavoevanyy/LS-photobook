ul.album-photo__list
    each val in obj
        li(class='album-photo__item')
            .album-photo__top
                .album-photo__img(style="background:url(/photo/"+val.photoId+") no-repeat")
                    .img-hover
                        a(href='#' class='hover-link')
                            .img-hover__block
                                svg(role="img" class="svg")
                                    use( xlink:href="/assets/img/sprite.svg#loupe")
            .world__down
                .world__down-inf
                    .user__avatar.small
                        .user__avatar-img.small(style="background-image:url('"+val.userPhoto+"');")
                            .avatar-hover
                                a(href='' class='avatar-hover__link')
                                    .avatar-hover__block
                                        svg(role="img" class="svg")
                                            use( xlink:href="/assets/img/sprite.svg#more")
                    .discription
                        .photo__discription #{val.desc}
                    .comments
                        a(href="#" class='comment-link')
                            .comments__note
                                .svg-wrap
                                    svg(role="img" class="svg")
                                        use( xlink:href="/assets/img/sprite.svg#comments")
                        .comments__quant #{val.comments.length}
                        a(href='#' class='likes-link')
                            .likes
                                .svg-wrap
                                    svg(role="img" class="svg")
                                        use( xlink:href="/assets/img/sprite.svg#like")
                        .likes__quant #{val.likes}
                .world__down-album
                    a(href='#' class="edit-link")
                        .svg-wrap
                            svg(role="img" class="svg")
                                use( xlink:href="/assets/img/sprite.svg#album")
                    .album-wrap
                        a(href='/album/' + val.albumId)
                            .album__name #{val.albumName}
